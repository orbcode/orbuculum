define dwtPOSTCNT
  if ($argc!=1)
    help dwtPOSTCNT
  else
    set *0xe000edfc |= 0x1000000
    if ($arg0==1)
      set *0xe0001000 |= (1<<22)
    else
      set *0xe0001000 &= ~(1<<22)
    end
  end
end
document dwtPOSTCNT
dwtPOSTCNT <0|1> Enable POSTCNT underflow event counter packet generation
end
